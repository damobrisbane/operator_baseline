{
  "kind": "ImageSetConfiguration",
  "apiVersion": "mirror.openshift.io/v1alpha2",
  "archiveSize": null,
  "storageConfig": {
    "registry": {
      "imageURL": "${STORAGE_CONFIG_URL}",
      "skipTLS": false
    }
  },
  "mirror": {
    "operators": [
      {
        "catalog": "${CATALOG}",
        "targetName": "${TARGET_NAME}",
        "targetTag": "${TARGET_TAG}",
        "packages": ${PKG_CHANNELS}
      }
    ]
  }
}
